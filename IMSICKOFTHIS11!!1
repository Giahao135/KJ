local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local imageLabel = Instance.new("ImageLabel")
imageLabel.Size = UDim2.new(0.10, 0, 0.20, 0)
imageLabel.Position = UDim2.new(0.45, 0, -0.20, 0) -- Start off-screen (top)
imageLabel.Image = getcustomasset(fileName)
imageLabel.BackgroundTransparency = 1
imageLabel.ImageTransparency = 1 -- Start fully transparent
imageLabel.Parent = screenGui

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Easing function (ease-out cubic)
local function easeOutCubic(t)
    return 1 - math.pow(1 - t, 3)
end

-- Fade-in and move effect
local duration = 2 -- Duration of the animation in seconds
local startTime = tick()
local startY = -0.10
local endY = 0.50

while tick() - startTime < duration do
    local elapsed = tick() - startTime
    local alpha = elapsed / duration
    local easedAlpha = easeOutCubic(alpha) -- Apply easing for movement

    -- Update position
    local newY = startY + (endY - startY) * easedAlpha
    imageLabel.Position = UDim2.new(0.45, 0, newY, 0)

    -- Update transparency (fade-in)
    imageLabel.ImageTransparency = 0.5 - alpha

    wait()
end

-- Ensure final position and transparency are correct
imageLabel.Position = UDim2.new(0.45, 0, 0.50, 0)
imageLabel.ImageTransparency = 0

-- Handle player respawn
local function onCharacterAdded()
    screenGui:Destroy()
end

-- Connect the respawn event
player.CharacterAdded:Connect(onCharacterAdded)

