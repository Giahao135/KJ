local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local s = Instance.new("ScreenGui")
s.Parent = playerGui

local url = "https://raw.githubusercontent.com/Giahao135/Image/refs/heads/main/KJIcon.png"
local fileName = "kj-icon.png"

if not isfile(fileName) then
    writefile(fileName, game:HttpGet(url))
end

-- Create GUI for image
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local imageLabel = Instance.new("ImageLabel")
imageLabel.Size = UDim2.new(0.10, -20, 0.22, -20)
imageLabel.Position = UDim2.new(0.45, 15, -0.20, 0) -- Start off-screen (top)
imageLabel.Image = getcustomasset(fileName)
imageLabel.BackgroundTransparency = 1
imageLabel.ImageTransparency = 1 -- Start fully transparent
imageLabel.Parent = screenGui

-- Create GUI for text
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local textGui = Instance.new("ScreenGui")
textGui.Parent = playerGui

local textLabel = Instance.new("TextLabel")
textLabel.Parent = textGui
textLabel.Position = UDim2.new(0.5, -200, 0.74, -50)
textLabel.Size = UDim2.new(0, 400, 0, 50)
textLabel.TextSize = 27
textLabel.TextColor3 = Color3.fromRGB(0, 0, 0) -- Text color
textLabel.Font = Enum.Font.Antique -- Font
textLabel.BackgroundTransparency = 1
textLabel.Text = "" -- Initially empty
textLabel.TextStrokeTransparency = 0
textLabel.TextStrokeColor3 = Color3.fromRGB(255, 0, 0) -- Stroke color

-- Easing function (ease-out cubic)
local function easeOutCubic(t)
    return 1 - math.pow(1 - t, 3)
end

-- Typing effect
local function typeText(text)
    local index = 1
    while index <= #text do
        textLabel.Text = string.sub(text, 1, index)
        index = index + 1
        wait(0.02)
    end
end

-- Fade-out effects
local function fadeOut()
    for i = 1, 20 do
        textLabel.TextTransparency = i / 20
        imageLabel.ImageTransparency = imageLabel.ImageTransparency + 0.05
        textLabel.TextStrokeTransparency = textLabel.TextStrokeTransparency + 0.05
        wait(0.05)
    end
end

-- Image fade-in and move effect
local duration = 1.2 -- Duration of the animation in seconds
local startTime = tick()
local startY = 0.20
local endY = 0.48

-- Start the text typing animation immediately
coroutine.wrap(function()
    typeText("It isn't...even worth it anymore...") -- Text content
    wait(1)
    fadeOut()
end)()

-- Image animation
while tick() - startTime < duration do
    local elapsed = tick() - startTime
    local alpha = elapsed / duration
    local easedAlpha = easeOutCubic(alpha)

    -- Update position and transparency
    local newY = startY + (endY - startY) * easedAlpha
    imageLabel.Position = UDim2.new(0.45, 15, newY, 0)
    imageLabel.ImageTransparency = 1 - easedAlpha

    wait()
end

-- Ensure final position and transparency are correct
imageLabel.Position = UDim2.new(0.45, 15, 0.48, 0)
imageLabel.ImageTransparency = 0

-- Player respawn handling
local function onCharacterAdded()
    screenGui:Destroy()
    textGui:Destroy()
end

player.CharacterAdded:Connect(onCharacterAdded)


--end time anim here
Wait(0.3)

--end time anim here

Anim:Stop() -- Stops the first animation

task.wait(0)

local AnimAnim2 = Instance.new("Animation")
AnimAnim2.AnimationId = "rbxassetid://16592787958"

local Anim2 = Humanoid:LoadAnimation(AnimAnim2)

local startTime = 2

Anim2:Play()
Anim2:AdjustSpeed(0)
Anim2.TimePosition = startTime
Anim2:AdjustSpeed(0.6)

    end

end

